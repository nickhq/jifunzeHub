        <activity android:name="io.nkossy.jifunzeHub.FlashCards.BiologyFlashCardActivity" />
        <activity android:name="io.nkossy.jifunzeHub.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
